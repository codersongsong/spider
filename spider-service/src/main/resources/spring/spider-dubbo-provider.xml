<?xml version="1.0" encoding="UTF-8" ?> 
<beans xmlns="http://www.springframework.org/schema/beans" 
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" 
			xsi:schemaLocation="http://www.springframework.org/schema/beans 
				http://www.springframework.org/schema/beans/spring-beans-2.5.xsd 
				http://code.alibabatech.com/schema/dubbo 
				http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
  <dubbo:consumer check="false" /> 
  <dubbo:registry protocol="zookeeper" address="${dubbo.run.registryAddress}" id="spiderDubboProvider" /> 
  <dubbo:protocol name="dubbo" port="-1" /> 
  
  <!--dubbo:service timeout="6000" group="${dubbo.group}" ref="demoFacadeServiceImpl" interface="com.gome.mammon.dubbo.DemoFacadeService" registry="spiderDubboProvider" /-->
   
</beans>
